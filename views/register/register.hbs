<h3>Register</h3>

<form action="/register" method="POST">
    <label>
        <p>
            Name: <input type="text" name="name" required>
        </p>
    </label>
    <label>
        <p>
            Password: <input type="password" name="password" required>
        </p>
    </label>

    <button type="submit">Register</button>
</form>